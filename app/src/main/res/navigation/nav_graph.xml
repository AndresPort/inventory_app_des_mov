<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/nav_graph"
    app:startDestination="@id/LoginFragment"
    >
    <fragment
        android:id="@+id/LoginFragment"
        android:name="com.andresport.app_inventory.view.fragment.LoginFragment"
        android:label="@string/fragment_login_window"
        tools:layout="@layout/fragment_login_window"
        >
        <action
            android:id="@+id/action_LoginFragment_to_InventarioFragment"
            app:destination="@id/inventarioFragment" />
    </fragment>
    <fragment
        android:id="@+id/inventarioFragment"
        android:name="com.andresport.app_inventory.view.fragment.InventarioFragment"
        android:label="Inventario"
        tools:layout="@layout/fragment_inventario">

    </fragment>



</navigation>